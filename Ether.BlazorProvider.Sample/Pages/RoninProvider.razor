@page "/roninprovider"
@using System.Numerics
@using Ether.BlazorProvider

@implements IDisposable
@inject IEtherProviderRegistry _etherProvider;

<PageTitle>Ether Provider (Ronin)</PageTitle>

    <ProviderSummary ProviderName="Ronin" Provider="@_myProvider"/>

@code {

    private IJsonRpcProvider? _myProvider;

    protected override async Task OnInitializedAsync()
    {
        _myProvider = await _etherProvider.GetProvider("ronin");
        await base.OnInitializedAsync();
    }

    public void Dispose()
    {
    }
}
